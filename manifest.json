{
  "manifest_version": 3,
  "name": "Chrome Extension",
  "version": "1.0.0",
  "description": "A Chrome extension with popup built using Svelte",
  "action": {
    "default_popup": "index.html",
    "default_title": "Svelte Popup"
  },
  "icons": {
    "16": "icons/app-icon@16.png",
    "48": "icons/app-icon@48.png",
    "128": "icons/app-icon@128.png"
  }
}